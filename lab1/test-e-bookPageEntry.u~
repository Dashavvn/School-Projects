#include "labfunctions.h"

test suite bookPageEntry
{
tests:
  test simple
  {
    check (bookPageEntry("The Chamber", "John Grisham")) expect ==
   "The Chamber . . . . . . . . . . . . . . . . . . . . . . . John Grisham";

check (bookPageEntry("Dashawn", "Dashawn Meredith")) expect ==
   "Dashawn . . . . . . . . . . . . . . . . . . . . . . .  Dashawn Meredit";
}
}
